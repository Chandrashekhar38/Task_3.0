create table Customers
(
    customer_id         varchar(20) primary key,
    customer_name       varchar(100),
    phone               varchar(30),
    address             varchar(100),
    city                varchar(50),
    state               varchar(20),
    postal_code         varchar(20),
    country             varchar(30)
);

