--1).  Basic SELECT
select * from customers;

select customer_id, customer_name, phone from customers;

select customer_name, phone address from customers;

select customer_name, address, city, state state from customers;

select customer_id, country state from customers;

select customer_id, postal_code state from customers;


--2). Filtering with WHERE
select * from customers where city = 'NYC';

select * from customers where city = 'Paris;

select * from customers where city = 'Madrid';

select * from customers where city = 'Brickhaven';

select * from customers where country = 'USA';

select * from customers where country = 'France';

select * from customers where country = 'Australia';

select * from customers where country =  'UK';

--3). Using AND / OR
select * from customers where country = 'USA' and city = 'Newark';

select * from customers where country = 'USA' or city = 'Newark';

select * from customers where country = 'France' and city = 'Paris';

select * from customers where country = 'France' or city = 'Paris';

select * from customers where country = 'UK' and  city = 'London';

select * from customers where country = 'UK' or  city = 'London';



--4). Pattern Matching (LIKE)
select * from customers where customer_name like 'a%';

select * from customers where country like 'u%';

select * from customers where customer_name like '%a';

select * from customers where customer_name like '%c';



--5). Range Filtering (BETWEEN)

select * from customers where country between 'France' and 'Germany';

select * from customers where country between 'France' and 'UK';

select * from customers where city between 'London' and 'NYC';


--6). Sorting

select * from customers order by city asc; 

select * from customers order by city desc;

select * from customers order by  country asc;

select * from customers order by  country desc;

--7). LIMIT rows

select * from customers limit 10;

select * from customers where country = 'USA' limit 10;



--8). DISTINCT values

select distinct(country) from customers;

select distinct(city) from customers;

select distinct(Customer_name) from customers;

select distinct(state) from customers;
